struct Person
{
	let name: string = "";
	let age: int32 = 0;
	let health: int32 = 100;
	func SayHi() -> void
	{
		print("Hi my name is " + name);
	}
}
struct Weapon
{
	let magazineCapacity: int32 = 10;
	let recoilForce: int32 = 15;
}
impl Shootable[Weapon, Person]
{
	func Shoot() -> void
	{
		if (Weapon::magazineCapacity > 0)
		{
			Weapon::magazineCapacity--;
			if (recoilForce > 10)
			{
				Person::health -= 0.02 * Person::age;
			}
		}
	}
}
impl Defusable[Person, DefuseKit]
{
	func Defuse() -> void
	{
		/* Stuff */
	}
}
comp CounterTerrorist[Person, Weapon, RiotShield] {}
comp BombSquadGuy[Person, Weapon, DefuseKit] {}
comp Terrorist[Person, Weapon, C4Explosive] {}
func main()
{

	CounterTerrorist ct1;
	BombSquadGuy bombGuy;
	Terrorist badGuy;

	ct1.Shoot(); // Ok
	bombGuy.Shoot(); // Ok
	badGuy.Shoot(); // Ok

	bombGuy.Defuse(); // Ok
	ct1.Defuse(); // Error

}
